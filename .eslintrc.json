{
	"parser": "babel-eslint",
	"extends": "airbnb",
	"plugins": [
		"react",
		"jsx-a11y",
		"import"
	],
	"rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/prefer-stateless-function": [0],
      "react/sort-comp": [0],
      "import/no-extraneous-dependencies": [0],
      "class-methods-use-this": [0],
      "jsx-a11y/aria-role": [0]
	}
}